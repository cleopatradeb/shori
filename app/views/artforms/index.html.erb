<p id="notice"><%= notice %></p>

<h1>Listing Artforms</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artforms.each do |artform| %>
      <tr>
        <td><%= link_to 'Show', artform %></td>
        <td><%= link_to 'Edit', edit_artform_path(artform) %></td>
        <td><%= link_to 'Destroy', artform, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artform', new_artform_path %>
