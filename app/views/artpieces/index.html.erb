<p id="notice"><%= notice %></p>

<h1>Listing Artpieces</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artpieces.each do |artpiece| %>
      <tr>
        <td><%= link_to 'Show', artpiece %></td>
        <td><%= link_to 'Edit', edit_artpiece_path(artpiece) %></td>
        <td><%= link_to 'Destroy', artpiece, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Artpiece', new_artpiece_path %>
